UPSTREAM_REPO=
echo "Building images**********************************************************"
sudo docker build manifest_A/ --tag=manifest_A
sudo docker build manifest_B/ --tag=manifest_b

echo "Tagging manifests**********************************************************"
sudo docker tag manifest_a asmacdo/trash-fixture-4:manifest:a
sudo docker tag manifest_b asmacdo/trash-fixture-4:manifest_b

echo "push to dockerhub**********************************************************"
sudo docker push asmacdo/trash-fixture-4


